    <TrimmerRootAssembly>True</TrimmerRootAssembly>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\commandline\CommandLineParser\CommandLineParser.csproj" />
  </ItemGroup>

</Project>
